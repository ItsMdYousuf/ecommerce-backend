{
   "version": 2,
   "builds": [
      {
         "src": "./index.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "/",
         "methods": [
            "GET,HEAD,PUT,PATCH,POST,DELETE"
         ]
      }
   ],
   "rewrites": [
      {
         "source": "/",
         "destination": "/api/index.js"
      }
   ]
}